{{#if valid}} 
    <h1>Profile of USER</h1>
    <p>Name: <strong>{{name}}</strong></p>
    <p>Lastname: <strong>{{lastname}}</strong></p>
{{else}}
    you are not auth
{{/if}}

<br>

{{#each pets}}
    <li>{{this}}</li>
{{/each}}

<br>

{{#each parents}}
    {{#each this}}
    <li>{{this}}</li>
    {{/each}}
{{/each}}

<br>

{{#each parents}}
    {{#each this as |val key|}}
        {{key}} : {{val}} <br>
    {{/each}}
{{/each}}

